{
    "name": "baaaaane/image-uploader",
    "description": "Image uploader",
    "type": "Library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arianne Nicole Batac",
            "email": "anebatac@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/container": "^5.7",
        "illuminate/console": "5.6.*|5.7.*|5.8.*",
        "illuminate/http": "5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.6.*|5.7.*|5.8.*",
        "symfony/var-dumper": "^4.1.1",
        "spatie/image-optimizer": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Baaane\\ImageUploader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Baaane\\ImageUploader\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
